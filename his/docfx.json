{
  "build": {
    "content": [
      {
        "files": [
          "**/**.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.gif",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/host-integration-server/breadcrumb/toc.json",
      "layout": "Conceptual",
      "titleSuffix": "Host Integration Server",
      "uhfHeaderId": "MSDocsHeader-His",
      "extendBreadcrumb": true
    },
    "fileMetadata": {
      "author": {"troubleshooting/*.md": "gplarsen"},
      "ms.author": {"troubleshooting/*.md": "plarsen"},
      "titleSuffix": {"troubleshooting/*.md": "Host Integration Server"}
    },
    "template": [],
    "dest": "his",
    "markdownEngineName": "markdig"

  }
}
